<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="合同管理" android:textSize="22sp"/>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:text="合同编号" android:layout_width="100dp"
			android:layout_height="wrap_content" android:textSize="18sp" />
		<TextView android:text="合同名称" android:layout_width="100dp"
			android:layout_height="wrap_content" android:textSize="18sp" />
		<TextView android:text="所属项目" android:layout_width="100dp"
			android:layout_height="wrap_content" android:textSize="18sp" />
	</LinearLayout>
	<ListView android:id="@+id/contractListView"
		android:layout_width="fill_parent" android:layout_height="wrap_content" android:scrollbars="none"/>
</LinearLayout>
